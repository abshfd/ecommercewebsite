.navbar{
	display:flex;
	flex-direction:row;
	align-items:baseline;
}
.navbar{
	display:block;
	align-items:center;

	padding:20px;
}
nav{
	flex: 1;
	text-align:right;
}
nav ul{
	display:inline-block;
	list-style-type:none;
}
nav ul li{
	display:inline-block;
	float:left;
	margin-right:30px;
}
a{
	display:inline-block;
	text-decoration:none;
	color:white;
	letter-spacing:2px;
	float:left;
	font-size:15px;
	margin:0 10px;
}
p{
	color:black;
}
