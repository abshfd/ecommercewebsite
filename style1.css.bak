.navbar{
	display:flex;
	flex-direction:row;
	align-items:baseline;
}
.navbar{
	display:block;
	align-items:center;

	padding:20px;
}
nav{

	flex:1;
	text-align:right;
}
nav ul{
	display:inline-block;
	list-style-type:none;
}
nav ul li{
	display:inline-block;
	float:;
	margin-right:30px;
}
nav ul li a{
	display:inline-block;
	text-decoration:none;
	color:white;
	letter-spacing:2px;
	padding:7px 13px;
	border-radius:3px;
	float:left;
	font-size:15px;
	margin:0 10px;
}
p{
	color:black;
}
