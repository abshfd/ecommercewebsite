*{
	margin:0;
	padding:0;
	box-sizing:border-box;
}
header{
	background:black;
	position:fixed;
	left:0px;
	right:0px;
	width:1350px;
	height:140px;
	
	font-size:0.9em;
	text-decoration:none;
	color:white;
}
.cart{
	position:fixed;
	top:0;
	right:-0;
	width:360px;
	min-height:60vh;
	padding:20px;
	background: white;
	box-shadow: -2px 0 4px hsl(0 4% 15% / 10%);
	transition: 0.7s;
}

.cart.active{
	right:0;
}
.cart-title{
	color:black;
	text-align:center;
	font-size: 1.5rem;
	font-weight:600;
	margin-top: 2rem;
}
.cart-box{
	display:grid;
	grid-template-columns:32% 50% 18%;
	align-items:center;
	gap:1rem;
	margin-top: 1rem;
}
.cart-img{
	width:100px;
	height:100px;
	object-fit:contain;
	padding:10px;
}
.detail-box{
	color:black;
	display:grid;
	row-gap:0.5rem;
}

.cart-product-title{
	font-size: 1.2rem;
}

.cart-price{
	font-weight:700;
}

.cart-quantity{
	border: 1px solid black;
	outline-color:red;
	width:2.4rem;
	height:2rem;
	text-align:center;
	font-size:1rem;
}

.cart-remove{
	color:black;
	font-size:20px;
	cursor:pointer;
	color:red;
}

.total{
	display:flex;
	justify-content: flex-end;
	margin-top: 1.5rem;
	border-top:1px solid black;
}

.total-title{
	color:black;
	font-size:1rem;
	font-weight:600;
}

.total-price{
	color:black;
	margin-left:0.5rem;
}

.btn-buy{
	display:flex;
	margin: 1.5rem auto 0 auto;
	padding: 12px 20px;
	border:none;
	background:red;
	color:white;
	font-size:1rem;
	font-weight: 500;
	cursor:pointer;
}

.btn-buy:hover{
	background:brown;
}

#close-cart{
	color:black;
	text-align:right;
	font-size:20px;
	margin:10px;
	position:absolute;
	top:1rem;
	right:0.8rem;
	cursor:pointer;
}
.k1:hover{
	flex-basis:100%;
	color:white;
	transform: translateY(-10px);
}
.k1:hover{
	background-color:red;
	transition:transform 0.5s;
}
.k2:hover{
	flex-basis:100%;
	color:white;
	transform: translateY(-10px);
}
.k2:hover{
	background-color:blue;
	transition:transform 0.5s;
}
.k3:hover{
	flex-basis:100%;
	color:white;
	transform: translateY(-10px);
}
.k3:hover{
	background-color:aqua;
	transition:transform 0.5s;
}
.k4:hover{
	flex-basis:100%;
	color:white;
	transform: translateY(-10px);
}
.k4:hover{
	background-color:green;
	transition:transform 0.5s;
}	
