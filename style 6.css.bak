*{
	margin:0;
	padding:0;
}
body{
	background:radial-gradient(brown,silver,white);
	border-style:groove;
}
div{
	display:inline-block;
}
h1{
	font-size:40px;
	padding:50px;
	padding-left:600px;
	color:green;
}


.r1{
	width:20%;
	background-color:radial-gradient(white,silver,yellow);
	border-radius:6px;
	margin :140px;
	transform:translateY(-10px);
}
.r1:hover{
	transition:.5s;
	box-shadow:2px 2px 10px black;
}
	
.image img{
	width:100%;
	border-top-right-radius:6px;
	border-top-left-radius:6px;
	height:300px;
	object-fit:cover;
}
.title{
	float:right;
	left:20px;
	margin:60px;
	font-size:20px;
}
.des{
	text-align:center;
	margin-left:50px;
	padding-left:20px;
	
}
button{
	margin-top:30px;
	margin-bottom:30px;
	font-size:20px;
	background-color:white;
	border: 1px solid black;
	padding:6px;
	border-radius:5px;
	transform:translateY(-10s);
}
button:hover{
	background-color:black;
	color:white;
	transition:.5s;
}


